<simulation>
	<general>
		<version>v0.1.0</version>
		<random-seed>*</random-seed>
		<instances>1</instances>
		<write-state-histogram>true</write-state-histogram>
		<write-lineage-map>true</write-lineage-map>
		<write-state>true</write-state>
		<write-metadata>true</write-metadata>
		<write-interval>false</write-interval>
		<path>/Users/dbborens/state/jeSLIME/</path>
		<date-stamp>true</date-stamp>
		<max-step>100001</max-step>
		<output-frames>
			<auto mode="decilog" />
		</output-frames>
	</general>
	<geometry>
		<class>HexArena</class>
		<width>16</width>
		<height>16</height>
	</geometry>
	<processes>
		<time-process>
			<id>0</id>
			<class>PopulationTrack</class>
			<period>1</period>
		</time-process>
		<cell-process>
			<id>100</id>
			<class>Scatter</class>
			<period>0</period>
			<types>2</types>
			<tokens>2</tokens>
		</cell-process>
		<cell-process>
			<id>200</id>
			<class>DivideAnywhere</class>
			<period>1</period>
		</cell-process>
		<cell-process>
			<id>300</id>
			<class>NeighborSwap</class>
			<period>10</period>
		</cell-process>
	</processes>
</simulation>
