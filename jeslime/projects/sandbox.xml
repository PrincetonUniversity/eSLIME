<simulation>
	<general>
		<version>v0.1.3</version>
		<random-seed>*</random-seed>
		<instances>1</instances>
		<write-state-histogram>true</write-state-histogram>
		<write-lineage-map>true</write-lineage-map>
		<write-state>true</write-state>
		<write-metadata>true</write-metadata>
		<write-interval>false</write-interval>
		<write-fix-time>true</write-fix-time>
		<path>/Users/dbborens/state/jeSLIME/</path>
		<date-stamp>true</date-stamp>
		<max-step>10000001</max-step>
		<output-frames>
			<auto mode="decilog" />
		</output-frames>
	</general>
	<geometry>
		<class>HexArena</class>
		<width>20</width>
		<height>20</height>
	</geometry>
	<processes>
		<time-process>
			<id>0</id>
			<class>SimpleGillespie</class>
			<period>1</period>
		</time-process>
		<cell-process>
			<id>50</id>
			<class>Scatter</class>
			<cell-descriptor>
				<class>FissionCell</class>
				<state>sequential</state>
				<initial-fitness>0.5</initial-fitness>
				<threshold>1.0</threshold>
			</cell-descriptor>
			<active-sites>
				<mode>list</mode>
				<coordinate x="5" y="5" />
			</active-sites>
			<period>0</period>
			<types>1</types>
			<tokens>1</tokens>
		</cell-process>
		<cell-process>
			<id>100</id>
			<class>Scatter</class>
			<cell-descriptor>
				<class>FissionCell</class>
				<state>sequential</state>
				<initial-fitness>0.5</initial-fitness>
				<threshold>1.0</threshold>
			</cell-descriptor>
			<period>0</period>
			<types>4</types>
			<tokens>3</tokens>
		</cell-process>
		<cell-process>
			<id>150</id>
			<class>UniformBiomassGrowth</class>
			<delta>0.001</delta>
			<defer>false</defer>
		</cell-process>
		<cell-process>
			<id>175</id>
			<class>UniformBiomassGrowth</class>
			<delta>0.5</delta>
			<defer>false</defer>
			<active-sites>
				<mode>list</mode>
				<coordinate x="5" y="5" />
			</active-sites>			
		</cell-process>
		<cell-process>
			<id>200</id>
			<class>DivideAnywhere</class>
		</cell-process>
		<cell-process>
			<id>300</id>
			<class>NeighborSwap</class>
			<period>10</period>
		</cell-process>
	</processes>
	<visualizations>
		<state-map>
			<edge-length>10</edge-length>
			<labels>false</labels>
			<format>png</format>
		</state-map>
	</visualizations>
</simulation>