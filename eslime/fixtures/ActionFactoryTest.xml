<fixture>

    <null/>

    <mock/>

    <die/>

    <trigger>
        <behavior>test</behavior>
        <target>
            <class>self</class>
        </target>
    </trigger>

    <clone>
        <target>
            <class>vacant-neighbors</class>
            <max>1</max>
        </target>
    </clone>

    <expand>
        <behavior>expand</behavior>
    </expand>

    <expand-to>
        <behavior>expand-to</behavior>
        <target>
            <class>vacant-neighbors</class>
            <max>1</max>
        </target>
    </expand-to>
    
    <swap>
        <behavior>swap</behavior>
        <target>
            <class>vacant-neighbors</class>
            <max>1</max>
        </target>
    </swap>

    <adjust-health>
        <delta>0.5</delta>
    </adjust-health>

    <stochastic-choice>
        <option weight="1.0">
            <null/>
        </option>
    </stochastic-choice>
</fixture>